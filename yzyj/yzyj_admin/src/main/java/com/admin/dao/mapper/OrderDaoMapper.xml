    <select id="searchOrder" parameterType="Order" resultType="com.admin.model.Order">
        select o.*,device.merchant_name,device.device_name,z.REALNAME from t_order o , t_device_list device, Z_AGENCY_PERSON z where 1=1
        <if test="orderNumber != null and orderNumber != ''">
            AND o.ORDER_NUMBER= #{orderNumber}
        </if>
        <if test="lklMerchantCode != null and lklMerchantCode != ''">
            AND device.LKL_MERCHANT_CODE= #{lklMerchantCode}
        </if>
        <if test="merchantYunId != null and merchantYunId != ''">
            AND o.MERCHANT_YUN_ID= #{merchantYunId}
        </if>
        <if test="strSdate != null and strSdate != ''">
            AND TO_CHAR(o.CREATE_TIME,'yyyy-mm-dd hh24:mi:ss') &gt;= #{strSdate}
        </if>
        <if test="strEdate != null and strEdate != ''">
            AND TO_CHAR(o.CREATE_TIME,'yyyy-mm-dd hh24:mi:ss') &lt;= #{strEdate}
        </if>
        <if test="orderType != null and orderType != ''">
            AND o.ORDER_TYPE= #{orderType}
        </if>
        <if test="payState != null and payState != ''">
            AND o.PAY_STATE= #{payState}
        </if>
        <if test="payType != null and payType != ''">
            AND o.PAY_TYPE= #{payType}
        </if>
        <if test="searchType == 2">
            AND o.AGENT_UNIQUE = #{agentUnique}
        </if>
        and o.AGENT_UNIQUE = z.AGENT_UNIQUE
        and device.device_unique = o.device_unique  order by o.CREATE_TIME desc
    </select>

    <select id="getOrderByNumber" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        q.*,
        w.REFERNUMBER,
        w.TRADE_TYPE,
        w.BATCHBILLNO,
        w.SYSTRACENO,
        e.LKL_MERCHANT_CODE,
        e.LKL_TERMINAL_CODE,
        e.MERCHANT_NAME,
        e.MERCHANT_YUN_PAY_ACCOUNT,
        r.REALNAME
        from T_ORDER q,T_PAY_INFO_LIST w,T_DEVICE_LIST e,Z_AGENCY_PERSON r
        where q.ORDER_NUMBER = w.OUT_TRADE_NO
        and q.DEVICE_UNIQUE = e.DEVICE_UNIQUE
        and q.AGENT_UNIQUE = r.AGENT_UNIQUE
        and q.ORDER_NUMBER = #{orderNumber,jdbcType=VARCHAR}
    </select>

